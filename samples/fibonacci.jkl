-- Naive fibonacci using recursion.
(def (fib n)
  (if (< n 2)
    n
    (+
      (fib (- n 1))
      (fib (- n 2))
      -- (fib (- n 2))
    )
  )
)

-- Test the first 10 inputs.
(for (i 10)
  (print i (fib i))
)

